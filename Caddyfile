{
	order forward_proxy first
	auto_https off
}
:443 , hk.martify.eu.cc:443 {
	tls /etc/nginx/ssl/fullchain.cer /etc/nginx/ssl/key.pem
	route {
		forward_proxy {
			basic_auth 3UUdxN qF7yvTpF
			hide_ip
			hide_via
			probe_resistance
		}
		forward_proxy {
			basic_auth uM8Jp HWCMnUnJ
			hide_ip
			hide_via
			probe_resistance
		}
		reverse_proxy https://pan.ymxx.net {
			header_up Host {upstream_hostport}
			header_up X-Forwarded-Host {host}
		}
	}
}
